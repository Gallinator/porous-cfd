FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

porousFilter{
    type explicitPorositySource;

    explicitPorositySourceCoeffs{
        selectionMode cellZone;
        cellZone mesh;
        type DarcyForchheimer;

        d   (5e7 1000 1000);
        f   (0 0 0);

        coordinateSystem
        {
            e1 (0 0.70710678 0.70710678);
            e2 (1 0 0);
        }
    }
}